
// print number
for(let i = 0 ; i< 10; i++){
    console.log(i)
}

// sum of numbers in array 
//Write a loop that prints the numbers from 1 to 10.
function sumArray(arr){
    let sum = 0
    for(let i = 0; i< 10;i++){
        sum += arr[i]
    }
    return arr
}
console.log(sumArray)

//3. Reverse an array
//Write a function that reverses an array using a loop. beacuse it resvers than we have used push
// resverses mean found lenght or mean first or last 
function reverseArray(arr){
    let reverses = []
    for(let i =0 ; i < arr.length; i++){
        reverses.push(arr[i])
    }
    return reverses
}
console.log(reverseArray)

